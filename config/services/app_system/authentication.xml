<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\ShopwareDpdApp\AppSystem\Authenticator\Authenticator"
                 id="bitbag.shopware_dpd_app.app_system.authenticator.authenticator">

            <argument type="string" key="$appSecret">%env(APP_SECRET)%</argument>
        </service>

        <service class="BitBag\ShopwareDpdApp\AppSystem\Authenticator\OAuthAuthenticator"
                 id="bitbag.shopware_dpd_app.app_system.authenticator.oauth_authenticator"
        />
    </services>
</container>
