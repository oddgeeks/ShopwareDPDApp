<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="BitBag\ShopwareDpdApp\Controller\OrderController"
                 id="bitbag.shopware_skeleton_app.controller.order"
        >
            <argument type="service" id="bitbag.shopware_app_system.repository.shop_repository" />
            <argument type="service" id="shopware.repository.order" />
            <argument type="service" id="shopware.repository.product" />
            <argument type="service" id="translator" />

            <tag name="controller.service_arguments"/>
        </service>
    </services>
</container>
