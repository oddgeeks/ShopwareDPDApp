{
    "name": "bitbag/shopware-dpd-app",
    "type": "project",
    "license": "MIT",
    "description": "DPD integration for Shopware",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "bitbag/shopware-app-system-bundle": "dev-master",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "bitbag/dpd-pl-api-php": "^1.0.",
        "guzzlehttp/guzzle": "^7.4",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/asset": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/event-dispatcher": "^5.4",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/monolog-bundle": "^3.7",
        "symfony/security-bundle": "^5.4",
        "symfony/translation": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/extra-bundle": "^3.3",
        "twig/twig": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        },
        "platform": {
            "php": "8.0"
        }
    },
    "autoload": {
        "psr-4": {
            "BitBag\\ShopwareDpdApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BitBag\\ShopwareDpdApp\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.4"
        }
    },
    "require-dev": {
        "bitbag/coding-standard": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "pact-foundation/pact-php": "^7.1",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-symfony": "^3.1",
        "roave/security-advisories": "dev-latest",
        "symfony/maker-bundle": "^1.36",
        "vimeo/psalm": "^4.19"
    },
    "minimum-stability": "stable"
}
