{% import "include/repository/shipping-method.twig" as shippingMethodRepository %}
{% import "include/defaults.twig" as defaultsService %}

{% set shippingMethod = shippingMethodRepository.getShippingMethod(defaultsService.shippingMethodKey) %}

{% if shippingMethod is not null %}
    {% do services.writer.upsert('shipping_method', [
        {
            id: shippingMethod.id,
            active: false
        }
    ]) %}
{% endif %}
